-- Check the specific errors causing the blockage
SET LINESIZE 200 PAGESIZE 100
COL apply_name FORMAT a20
COL error_message FORMAT a60
COL source_commit_scn FORMAT 999999999999999

SELECT apply_name, 
       source_commit_scn, 
       message_number, 
       error_creation_time, 
       error_message 
FROM   dba_apply_error 
ORDER BY error_creation_time DESC;


-- Check the health of the Inbound Server processes
COL component_name FORMAT a20
COL state FORMAT a20
COL event FORMAT a40

SELECT component_name, 
       state, 
       count(*) as process_count, 
       event 
FROM   v$gg_apply_server 
WHERE  apply_name = 'UPSIREP' 
GROUP BY component_name, state, event
ORDER BY 1;

SET LINESIZE 200 PAGESIZE 100
COL apply_name FORMAT a20
COL error_message FORMAT a60
COL source_commit_scn FORMAT 999999999999999

SELECT apply_name, 
       source_commit_scn, 
       message_number, 
       error_creation_time, 
       error_message 
FROM   dba_apply_error 
ORDER BY error_creation_time DESC;



