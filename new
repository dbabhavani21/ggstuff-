  SELECT 'SPR00DBO','T_FSITM_IN_THE_MONEY_FS' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPCF_PRODUCT_CA_STATEMENT_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPCL_PRODUCT_CONTROL_FS' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPEF_PRTC_EXERCISE_OF' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPPF_PRODUCT_PRIOR_CURR_FS' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPRD_PROD_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPRP_PRTC_RSTC_PAYMENT_OF' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPUR_PUR_TRX_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSSGF_SHRPOOL_GRANT_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSSPD_SHARE_POOL_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSSSF_SHRPOOL_SUMMARY_FS' FROM dual UNION ALL
