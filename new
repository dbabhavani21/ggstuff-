SELECT
    tablespace_name,
  2    3      ROUND(SUM(bytes)/1024/1024/1024, 2) AS total_gb,
    ROUND(SUM(CASE WHEN status = 'ACTIVE' THEN bytes ELSE 0 END)/1024/1024/1024, 2) AS active_gb,
    ROUND(SUM(CASE WHEN status = 'UNEXPIRED' THEN bytes ELSE 0 END)/1024/1024/1024, 2) AS unexpired_  4  gb,
    ROUND(SUM(CASE WHEN status = 'EXPIRED' THEN bytes ELSE 0 END)/1024/1024/1024, 2) AS expired_gb
FROM dba_undo_extents
GROUP BY tablespace_name;  5    6    7    8

TABLESPACE_NAME                  TOTAL_GB  ACTIVE_GB UNEXPIRED_GB EXPIRED_GB
------------------------------ ---------- ---------- ------------ ----------
UNDOTBS2                           137.32      85.34        48.46       3.52
UNDOTBS1                           289.89        .01       221.19      68.69
UNDOTBS3                           497.59          0       203.93     293.66
UNDOTBS4                           503.98          0        91.79     412.19
