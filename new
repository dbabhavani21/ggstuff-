  SELECT 'SPR00DBO','T_FPDAC_DATE_CNTL' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSBFF_BLACKOUT_RULE_FILTER_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSBGF_BLACKOUT_GRANT_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSBPD_TAX_BKT_RNG_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSBRD_BLACKOUT_RL_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCTS_CLIENT_TRUST_SWEEP_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSDID_DIVIDEND_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSFXD_FRN_EXCH_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSGVD_GRNT_VEST_DIV_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSLRD_LOT_LVL_RSTC_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSLVF_TAX_ALLOC_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSOCF_ORD_CANCEL_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPAD_PARTICIPANT_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPDD_PRTC_DIST_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPGD_PRTC_GRANT_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPGF_PERF_GRANT_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPID_PRICE_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPPD_END_D_GTE_PROC_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPPD_PERF_PERIOD_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPPV_PERF_PERIOD_VEST_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPRG_PRTC_G_TAX_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPSF_PRODUCT_STATEMENT_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPTA_PRTC_TAX_ASGN_DTL_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPTB_PRTC_TAX_ASGN_DTL_FS' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPVD_PRTC_VEST_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPVF_END_D_ACTIVE' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSPVF_PERF_VEST_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSSED_SEC_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSTMF_TAX_METH_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSTPD_TAX_ASGT_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSWID_WIRE_INSN_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','VESTING_ACTIVITY_TRX_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSECF_ENROLL_CTRB_F' FROM dual
)
SELECT
  lst.owner,
  lst.table_name,
  ROUND(NVL(tab.bytes,0)/1024/1024,2) AS table_mb,
  ROUND(NVL(idx.bytes,0)/1024/1024,2) AS index_mb,
  ROUND(NVL(lob.bytes,0)/1024/1024,2) AS lob_mb,
  ROUND((NVL(tab.bytes,0)+NVL(idx.bytes,0)+NVL(lob.bytes,0))/1024/1024,2) AS total_mb
FROM lst
LEFT JOIN tab ON tab.owner = lst.owner AND tab.table_name = lst.table_name
LEFT JOIN idx ON idx.owner = lst.owner AND idx.table_name = lst.table_name
LEFT JOIN lob ON lob.owner = lst.owner AND lob.table_name = lst.table_name
ORDER BY total_mb DESC;

























