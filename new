    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'TRX_D'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSGTF_GRANT_TRX_OF')
    UNION ALL
    SELECT t.owner, t.table_name, 'UPD_TMST', CASE WHEN c.column_name IS NOT NULL THEN 'PRESENT' ELSE '>>> MISSING <<<' END
    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'UPD_TMST'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSCAD_CORP_ACTION_D','T_FSCGD_CA_GRANT_D','T_FSCLD_CLIENT_D','T_FSDVD_DIVISION_D','T_FSGAD_GRANT_AGREE_D','T_FSPRD_PROD_D','T_FSSPD_SHARE_POOL_D','T_FSTSD_TERMINATION_SCHD_D','T_FSVSD_VESTING_SCHD_D')
    UNION ALL
    SELECT t.owner, t.table_name, 'YR_N', CASE WHEN c.column_name IS NOT NULL THEN 'PRESENT' ELSE '>>> MISSING <<<' END
    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'YR_N'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSDAD_DATE_D')
)
ORDER BY status DESC, owner, table_name;

