    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'STRT_D'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FPDAC_DATE_CNTL')
    UNION ALL
