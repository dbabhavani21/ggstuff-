SET LINESIZE 200
SET PAGESIZE 100
COL OWNER FORMAT A15
COL TABLE_NAME FORMAT A40
COL TABLE_GB FORMAT 999,990.999

SELECT s.owner, 
       s.segment_name AS table_name,
       ROUND(SUM(s.bytes)/1024/1024/1024, 3) AS table_gb
FROM dba_segments s
WHERE s.segment_type IN ('TABLE','TABLE PARTITION','TABLE SUBPARTITION')
AND (
    (s.owner = 'SPR00DBO' AND s.segment_name IN (
        'AWARD_DISTRIBUTION_D', 'BRKR_PLAN_CUSTODY_ACCT_D', 'BRKR_PRTC_D', 
        'CLNT_DOC_CALLOUTS_D', 'CLNT_DOC_PRODUCTS_D', 'CORP_ACTION_D', 
        'CORP_GRANT_TAX_D', 'CUSIP_PRICING_ADJ_D', 'DATE_D', 
        'DISTRIBUTION_FRQ_D', 'DISTRIBUTION_SCHD_D', 'ENDING_BALANCE_F', 
        'ENDING_BALANCE_IFRS_F', 'FED_INTEREST_RTE_D', 'FILE_LOAD_INFO_D', 
        'FISCAL_MONTH_D', 'GRANT_ACTIVITY_TRX_F', 'GRANT_AGREE_D', 
        'GRANT_EXPENSE_F', 'GRANT_TRANCHE_D', 'GRANT_TRANCHE_EXT_D', 
        'GRANT_VAL_ASSUMPTION_D', 'OFFERING_D', 'OVERRIDE_D', 
        'PARTICIPANT_D', 'PERFORMANCE_SCHD_D', 'PERF_GRANT_RATE_D', 
        'PERF_PERIOD_GRANT_D', 'PERF_PERIOD_VESTING_D', 'PLAN_D', 
        'PLAN_SPONSOR_D', 'PRODUCT_MAPPINGS_D', 'PROD_D', 
        'PRORATION_RL_D', 'PRTC_GRANT_CSTM_D', 'PRTC_GRANT_D', 
        'PRTC_GRANT_D_MV', 'PRTC_GRANT_D_PLAN_MV', 'PRTC_GRANT_EXT_D', 
        'PRTC_MOBILITY_D', 'PS_FAS_LEDGER_D', 'RPT_DIST_TRX_CTA_F', 
        'TERMINATION_SCHD_D', 'TRANCHE_EXPENSE_F', 'TRANCHE_VAL_ASSUMPTION_D', 
        'T_FSBFF_BLACKOUT_RULE_FILTER_F', 'T_FSBGF_BLACKOUT_GRANT_F', 
        'T_FSBPD_TAX_BKT_RNG_D', 'T_FSBRD_BLACKOUT_RL_D', 'T_FSDID_DIVIDEND_D', 
        'T_FSGVD_GRNT_VEST_DIV_F', 'T_FSLRD_LOT_LVL_RSTC_D', 'T_FSLVF_TAX_ALLOC_F', 
        'T_FSOCF_ORD_CANCEL_F', 'T_FSPRG_PRTC_G_TAX_F', 'T_FSPTA_PRTC_TAX_ASGN_DTL_F', 
        'T_FSPTB_PRTC_TAX_ASGN_DTL_FS', 'T_FSSED_SEC_D', 'T_FSTMF_TAX_METH_F', 
        'T_FSTPD_TAX_ASGT_D', 'VESTING_ACTIVITY_TRX_F', 'SPS_LOAD_ERRORS',
        'T_FSCPD_CLIENT_PLAN_D', 'T_FSPEF_PRTC_EXERCISE_OF', 'T_FSPRP_PRTC_RSTC_PAYMENT_OF',
        'T_FSPUR_PUR_TRX_F', 'BRKR_ERROR_F', 'BRKR_MSG_TRKR_D', 'DIVISION_D',
        'GRANT_ACTIVITY_F', 'GRANT_ACTIVITY_HIST_F', 'PRODUCT_CONTROL_FS',
        'SHARE_POOL_D', 'SPS_CYCLE_ERRORS', 'TABBED_DIST_F', 'TABBED_VEST_F',
        'T_FASE_FAS_ERROR', 'T_FSERR_ERROR', 'T_FSCSK_CASH_SWEEP_JOURNAL_F',
        'T_FSCST_CASH_SWEEP_TRX_F', 'T_FSAOF_APPROVED_OPTIONS_F', 'T_FSITM_IN_THE_MONEY_FS',
        'T_FSCTS_CLIENT_TRUST_SWEEP_D', 'T_FSECF_ENROLL_CTRB_F', 'T_FSFXD_FRN_EXCH_D',
        'T_FSPAD_PARTICIPANT_D', 'T_FSPDD_PRTC_DIST_D', 'T_FSPGD_PRTC_GRANT_D',
        'T_FSPGF_PERF_GRANT_F', 'T_FSPID_PRICE_D', 'T_FSPPD_END_D_GTE_PROC_D',
        'T_FSPPD_PERF_PERIOD_D', 'T_FSPPV_PERF_PERIOD_VEST_D', 'T_FSPSF_PRODUCT_STATEMENT_F',
        'T_FSPVD_PRTC_VEST_D', 'T_FSPVF_END_D_ACTIVE', 'T_FSPVF_PERF_VEST_F',
        'T_FSWID_WIRE_INSN_D', 'T_FSCWT_CTRB_WD_TRX_F', 'T_FPDAC_DATE_CNTL',
        'T_FSTDF_TRNF_DEP_F', 'T_FSPCF_PRODUCT_CA_STATEMENT_F', 'T_FSPCL_PRODUCT_CONTROL_FS',
        'T_FSPPF_PRODUCT_PRIOR_CURR_FS', 'T_FSSGF_SHRPOOL_GRANT_F', 'T_FSSSF_SHRPOOL_SUMMARY_FS',
        'T_FSETF_ENROLL_TRX_F', 'T_FSTRF_TERMINATION_RECAP_F', 'T_FSCTF_CASH_TRANSFER_F',
        'T_FSGTF_GRANT_TRX_OF', 'T_FSCAD_CORP_ACTION_D', 'T_FSCGD_CA_GRANT_D',
        'T_FSCLD_CLIENT_D', 'T_FSDVD_DIVISION_D', 'T_FSGAD_GRANT_AGREE_D',
        'T_FSPRD_PROD_D', 'T_FSSPD_SHARE_POOL_D', 'T_FSTSD_TERMINATION_SCHD_D',
        'T_FSVSD_VESTING_SCHD_D', 'T_FSDAD_DATE_D'
    ))
    OR
    (s.owner = 'GRR00DBO' AND s.segment_name IN (
        'GRK_AWARD_DISTRIBUTION_D', 'GRK_BRKR_PLAN_CUSTODY_ACCT_D', 'GRK_CSR_HIER_SETUP_D',
        'GRK_DIST_PAID_TRNF_REQ_D', 'GRK_DIST_PAID_TRNS_DTL_D', 'GRK_EST_TAX_D',
        'GRK_FILE_LOAD_INFO_D', 'GRK_GRANT_AGREE_D', 'GRK_GRANT_TRANCHE_D',
        'GRK_GRNT_VEST_DIV_F', 'GRK_PERF_GRANT_PED_D', 'GRK_PRTC_GRANT_D',
        'GRK_RETIREMENT_ELG_RULE_D', 'GRK_SHRPOOL_GRANT_F', 'GRK_SHRPOOL_SUMMARY_FS',
        'GRK_STOCK_PRICE_D', 'GRK_VEST_PED_SUMMARY_F', 'GRK_BRKR_ERROR_F',
        'GRK_BRKR_MOBILITY_F', 'GRK_BRKR_MSG_TRKR_D', 'GRK_DIST_PAID_TRX_D',
        'GRK_DIST_TAX_D', 'GRK_GRANT_ACTIVITY_F', 'GRK_GRANT_ACTIVITY_HIST_F',
        'GRK_MBLT_AWD_DST_TRCK_F', 'GRK_PRODUCT_CONTROL_FS', 'GRK_PRTC_BRKR_INFO_D',
        'GRK_TAX_METH_F'
    ))
)
GROUP BY s.owner, s.segment_name
ORDER BY table_gb DESC;





