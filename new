    SELECT t.owner, t.table_name, 'SNAPSHOT_D', CASE WHEN c.column_name IS NOT NULL THEN 'PRESENT' ELSE '>>> MISSING <<<' END
    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'SNAPSHOT_D'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSAOF_APPROVED_OPTIONS_F','T_FSITM_IN_THE_MONEY_FS')
    UNION ALL
    SELECT t.owner, t.table_name, 'START_D', CASE WHEN c.column_name IS NOT NULL THEN 'PRESENT' ELSE '>>> MISSING <<<' END
    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'START_D'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSCTS_CLIENT_TRUST_SWEEP_D','T_FSECF_ENROLL_CTRB_F','T_FSFXD_FRN_EXCH_D','T_FSPAD_PARTICIPANT_D','T_FSPDD_PRTC_DIST_D','T_FSPGD_PRTC_GRANT_D','T_FSPGF_PERF_GRANT_F','T_FSPID_PRICE_D','T_FSPPD_END_D_GTE_PROC_D','T_FSPPD_PERF_PERIOD_D','T_FSPPV_PERF_PERIOD_VEST_D','T_FSPSF_PRODUCT_STATEMENT_F','T_FSPVD_PRTC_VEST_D','T_FSPVF_END_D_ACTIVE','T_FSPVF_PERF_VEST_F','T_FSWID_WIRE_INSN_D')
