    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'DT_DIM_ID'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSPCF_PRODUCT_CA_STATEMENT_F','T_FSPCL_PRODUCT_CONTROL_FS','T_FSPPF_PRODUCT_PRIOR_CURR_FS','T_FSSGF_SHRPOOL_GRANT_F','T_FSSSF_SHRPOOL_SUMMARY_FS')
    UNION ALL
    SELECT t.owner, t.table_name, 'ENROLL_TRX_DT_DIM_ID', CASE WHEN c.column_name IS NOT NULL THEN 'PRESENT' ELSE '>>> MISSING <<<' END
    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'ENROLL_TRX_DT_DIM_ID'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSETF_ENROLL_TRX_F')
    UNION ALL
    SELECT t.owner, t.table_name, 'TRM_D', CASE WHEN c.column_name IS NOT NULL THEN 'PRESENT' ELSE '>>> MISSING <<<' END
    FROM dba_tables t LEFT JOIN dba_tab_columns c ON t.owner = c.owner AND t.table_name = c.table_name AND c.column_name = 'TRM_D'
    WHERE t.owner = 'SPR00DBO' AND t.table_name IN ('T_FSTRF_TERMINATION_RECAP_F')
