WITH tab AS (
  SELECT owner, segment_name AS table_name, SUM(bytes) AS bytes
  FROM dba_segments
  WHERE segment_type IN ('TABLE','TABLE PARTITION','TABLE SUBPARTITION')
  GROUP BY owner, segment_name
),
idx AS (
  SELECT i.table_owner AS owner, i.table_name, SUM(s.bytes) AS bytes
  FROM dba_indexes i
  JOIN dba_segments s
    ON s.owner = i.owner
   AND s.segment_name = i.index_name
  GROUP BY i.table_owner, i.table_name
),
lob AS (
  SELECT l.owner, l.table_name, SUM(s.bytes) AS bytes
  FROM dba_lobs l
  JOIN dba_segments s
    ON s.owner = l.owner
   AND s.segment_name = l.segment_name
  GROUP BY l.owner, l.table_name
),
lst AS (
  SELECT 'GRR00DBO' owner, 'GRK_BRKR_ERROR_F' table_name FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_BRKR_MOBILITY_F' FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_BRKR_MSG_TRKR_D' FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_GRANT_ACTIVITY_F' FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_GRANT_ACTIVITY_HIST_F' FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_MBLT_AWD_DST_TRCK_F' FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_PRODUCT_CONTROL_FS' FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_PRTC_BRKR_INFO_D' FROM dual UNION ALL
  SELECT 'GRR00DBO','GRK_TAX_METH_F' FROM dual UNION ALL
  SELECT 'GRR00DBO','BRKR_ERROR_F' FROM dual UNION ALL
  SELECT 'GRR00DBO','BRKR_MSG_TRKR_D' FROM dual UNION ALL
  SELECT 'GRR00DBO','DIVISION_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','GRANT_ACTIVITY_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','GRANT_ACTIVITY_HIST_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','PRODUCT_CONTROL_FS' FROM dual UNION ALL
  SELECT 'SPR00DBO','SHARE_POOL_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','SPS_CYCLE_ERRORS' FROM dual UNION ALL
  SELECT 'SPR00DBO','SPS_LOAD_ERRORS' FROM dual UNION ALL
  SELECT 'SPR00DBO','TABBED_DIST_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','TABBED_VEST_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FASE_FAS_ERROR' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSAOF_APPROVED_OPTIONS_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCAD_CORP_ACTION_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCGD_CA_GRANT_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCLD_CLIENT_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCPD_CLIENT_PLAN_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCSK_CASH_SWEEP_JOURNAL_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCST_CASH_SWEEP_TRX_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCTF_CASH_TRANSFER_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSCWT_CTRB_WD_TRX_F' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSDAD_DATE_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSDVD_DIVISION_D' FROM dual UNION ALL
  SELECT 'SPR00DBO','T_FSERR_ERROR' FROM dual UNION ALL
